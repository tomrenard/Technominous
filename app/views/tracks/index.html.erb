  <section class="gallery">
    <% @tracks.each do |track| %>
    <div class="item">
      <img src="<%= track.photo_link %>" alt="">
      <div class="item_overlay">
        <a href="<%= track.video_link %>" target="_blank"><%= track.artist %> - <%= track.title %></a>
      </div>
    </div>
    <% end %>
  </section>


